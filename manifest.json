{
  "manifest_version": 2,
  "name": "Dynamic Bookmarks",
  "description": "Updates bookmarks dynamically based on given regExp",
  "version": "0.3.1",
  "permissions": ["tabs", "bookmarks", "storage"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "images/icons8_Books_64.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icons8_Books_16.png",
    "32": "images/icons8_Books_32.png",
    "48": "images/icons8_Books_48.png",
    "64": "images/icons8_Books_64.png"
  }
}
